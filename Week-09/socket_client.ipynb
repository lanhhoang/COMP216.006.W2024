   "source": [
    "import socket\n",
    "\n",
    "host = \"localhost\"\n",
    "port = 12345\n",
    "address = (host, port)\n",
    "\n",
    "client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "client.connect(address)\n",
    "\n",
    "msg = \"hello\"\n",
    "client.send(msg.encode())\n",
    "ack = client.recv(1024).decode()\n",
    "print(ack)\n",
    "client.close()"
   ]
  }
